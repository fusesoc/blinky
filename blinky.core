targets:
  default: &default
    filesets : [rtl]
    toplevel: ["is_toplevel? (blinky)"]

  ac701:
    default_tool: vivado
    description : Xilinx Artix-7 FPGA AC701 Evaluation Kit
    filesets : [rtl, ac701]
    tools:
      vivado:
        part : xc7a200t-fbg676-2
    toplevel : blinky_ac701

  agilex5_dk:
    default_tool : quartus
    filesets : [rtl, agilex5_dk]
    parameters : [clk_freq_hz=100000000]
    tools:
      quartus:
        family : Agilex
        device : A5ED065BB32AE6SR0
    toplevel: blinky

  alchitry_au:
    default_tool: vivado
    description : SparkFun Alchitry Au, 100 MHz Xilinx Artix-7 with 256MB DDR3 RAM
    filesets: [rtl, alchitry_au]
    parameters: [clk_freq_hz=100000000]
    tools:
      vivado:
        part: xc7a35tftg256-1
    toplevel: blinky

  alchitry_au_plus:
    default_tool: vivado
    description : SparkFun Alchitry Au+, 100 MHz Xilinx Artix-7 with 256MB DDR3 RAM
    filesets: [rtl, alchitry_au_plus]
    parameters: [clk_freq_hz=100000000]
    tools:
      vivado:
        part: xc7a100tftg256-1
    toplevel: blinky

  alchitry_au_plus_io:
    default_tool: vivado
    description : SparkFun Alchitry Au+, 100 MHz Xilinx Artix-7 with 256MB DDR3 RAM (with Io Element Board)
    filesets: [rtl, alchitry_au_plus_io]
    parameters: [clk_freq_hz=100000000]
    tools:
      vivado:
        part: xc7a100tftg256-1
    toplevel: blinky

  alchitry_cu:
    default_tool: icestorm
    description: SparkFun Alchitry Cu, 100MHz iCE40-HX4K
    filesets: [rtl, proginfo, alchitry_cu]
    parameters: [clk_freq_hz=100000000]
    tools:
      icestorm:
        pnr: next
        nextpnr_options: [--hx8k, --package, cb132]
        arch: ice40
    toplevel: blinky

  alchitry_cu_io:
    default_tool: icestorm
    description: SparkFun Alchitry Cu, 100MHz iCE40-HX4K (with Io Element Board)
    filesets: [rtl, proginfo, alchitry_cu_io]
    parameters: [clk_freq_hz=100000000]
    tools:
      icestorm:
        pnr: next
        nextpnr_options: [--hx8k, --package, cb132]
        arch: ice40
    toplevel: blinky

  alhambra_II:
    default_tool : icestorm
    description : Alhambra II iCE40-HX4K based open-source hardware board
    hooks:
      post_run: [iceprog]
    filesets : [rtl, proginfo, alhambra_II]
    parameters : [clk_freq_hz=12000000]
    tools:
      icestorm:
        nextpnr_options : [--hx8k, --package, "tq144:4k"]
        pnr : next
    toplevel : blinky

  AnalogMax:
    default_tool : quartus
    filesets : [rtl, AnalogMax]
    parameters : [clk_freq_hz=12000000]
    tools:
      quartus:
        family : MAX 10
        device : 10M08SAU169C8G
    toplevel: blinky

  apf27:
    default_tool : ise
    filesets : [rtl, apf27]
    parameters : [clk_freq_hz=100000000]
    tools:
      ise:
        family  : Spartan3A
        device  : xc3s200a
        package : ft256
        speed   : -5
    toplevel : blinky

  apf51:
    default_tool : ise
    filesets : [rtl, apf51]
    parameters : [clk_freq_hz=92850000]
    tools:
      ise:
        family  : Spartan6
        device  : xc6slx9
        package : csg225
        speed   : -2
    toplevel : blinky

  arty_a7_35t:
    default_tool : vivado
    filesets : [rtl, arty_a7_35t]
